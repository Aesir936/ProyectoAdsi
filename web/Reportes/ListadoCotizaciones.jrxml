     `tbl_estados_cotizaciones` tbl_estados_cotizaciones INNER JOIN `tbl_cotizaciones` tbl_cotizaciones ON tbl_estados_cotizaciones.`id_estado_cotizacion` = tbl_cotizaciones.`id_estado_cotizacion`
